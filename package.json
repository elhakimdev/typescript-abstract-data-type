{
  "name": "@elhakimdev/typescript-adt",
  "version": "0.0.8",
  "description": "Experimental abstraction of data structure utils for javascript",
  "repository": {
    "type": "git",
    "url": "https://github.com/elhakimdev/typescript-abstract-data-type"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev --ui",
    "play": "jiti playground",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "docs:generate": "documentation build src/** -f md --github -o ./docs/@api/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "changelogen": "^0.5.7",
    "documentation": "^14.0.3",
    "eslint": "^8.57.1",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.6",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitepress": "1.0.0-rc.40",
    "vitest": "^1.6.0",
    "vue": "^3.5.12"
  }
}